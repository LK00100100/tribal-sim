{
  "name": "tribal-sim",
  "version": "1.0.0",
  "description": "tribal simulator game",
  "private": true,
  "scripts": {
    "test": "jest",
    "dev": "webpack --mode=development --watch",
    "build": "webpack --mode=production",
    "start:dev": "webpack-dev-server --mode=development --port=8888 --host 0.0.0.0",
    "start": "npm-run-all --parallel dev start:dev lint:watch",
    "ngrok": "ngrok http --host-header=rewrite 8888",
    "lint": "esw ./src/** ./test/** --color",
    "lint:watch": "npm run lint -- --watch",
    "lint:fix": "npm run lint -- --fix"
  },
  "author": "L. Keh: LK00100100@gmail.com",
  "devDependencies": {
    "eslint-watch": "^5.1.2",
    "jest": "^24.7.1",
    "ngrok": "^3.1.1",
    "npm-run-all": "^4.1.5",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14"
  },
  "dependencies": {
    "phaser": "^3.15.1",
    "terser": "^3.14.1"
  },
  "keywords": []
}
